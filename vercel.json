{
    "version": 2,
    "builds": [
        {
            "src": "app/main.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "app/main.py"
        }
    ],
    "env": {
        "DATABASE_URL": "@database_url",
        "BLOB_READ_WRITE_TOKEN": "@blob_read_write_token"
    }
}